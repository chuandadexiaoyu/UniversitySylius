<?xml version="1.0" encoding="UTF-8"?>


<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <mapped-superclass name="Sylius\Bundle\AddressingBundle\Model\Campus" table="sylius_campus">
        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="name" column="name" type="string" />

        <many-to-one field="university" target-entity="Sylius\Bundle\AddressingBundle\Model\UniversityInterface" inversed-by="campuses">
            <join-column name="university_id" referenced-column-name="id" />
        </many-to-one>

    </mapped-superclass>

</doctrine-mapping>
