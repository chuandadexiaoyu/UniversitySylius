<?xml version="1.0" encoding="UTF-8"?>


<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">

    <mapped-superclass name="Sylius\Bundle\AddressingBundle\Model\ChinaProvince" table="sylius_china_province">
        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="name" column="name" type="string" />

        <one-to-many field="cities" target-entity="Sylius\Bundle\AddressingBundle\Model\CityInterface" mapped-by="chinaProvince">
            <cascade>
            	<cascade-all/>
            </cascade>
        </one-to-many>
    </mapped-superclass>

</doctrine-mapping>